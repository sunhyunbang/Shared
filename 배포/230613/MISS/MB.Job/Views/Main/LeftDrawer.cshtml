  preUrl = preUrl.Replace("https:///", "");
  preUrl = preUrl.Replace("http:///", "");

  //ADD GTM 2023.06.01
  string strGoogleTagID = ViewBag.ServiceType.ToUpper() == "WEB" ? "GTM-PBFMJ9G" : "GTM-WRX4T44";
  string strCuid = MemberInfo.GetInstance.CUID;

  if (preUrl.Trim() != "")
  {
    if (preUrl.IndexOf("/") > 0)
    {
      preUrl = preUrl.Substring(0, preUrl.IndexOf("/"));
    }

    if (preUrl == "Recruit" || preUrl == "Specialist" || preUrl == "Lately")
    {
      // 일자리정보
      lastSelect = "1";
    }
    else if (preUrl == "JobGuide")
    {
      // 취업가이드
      lastSelect = "2";
    }
    else if (preUrl == "resume" || preUrl == "Resume" || preUrl == "Service")
    {
      // 개인회원
      lastSelect = "4";
    }
    else if (preUrl == "Advertiser" || preUrl == "AdRegistNew" || preUrl == "AdManage" || preUrl == "AdApplyList" || preUrl == "Common" || preUrl == "adRegistNew")
    {
      // 기업회원
      lastSelect = "5";
    }
    else
    {
      // 메인홈에서 진입시 ?mid= 값이 들어옴
      if (MemberInfo.GetInstance.IsBiz_Job == "Y")
      {
        // 기업회원
        lastSelect = "5";
      }
    }
  }
  else if (MemberInfo.GetInstance.IsBiz_Job == "Y")
  {
    // 기업회원
    lastSelect = "5";
  }
}
