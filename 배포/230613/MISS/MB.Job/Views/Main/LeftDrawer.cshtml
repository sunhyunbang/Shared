                      setCookie("osversioncheck", "Y", 1);
                    }
                    //location.href = 'tel:' + pTelNo;
                    return true;
                  }
                }
              }
              window.location = 'jscall://' + callString;
            } else {
              //location.href = 'tel:' + pTelNo;
              return true;
            }
          }
          else {
            //ANDROID_APP
            callString = '@@Action=FN_LM_TEL@@Param1=tel:' + pTelNo + '@@Param2=@@Param3=@@rURL=';
            if (strPalyStoreConfirm == "Y") {
              window.android.FN_LM_TEL(callString);
            }
            else if (strtStoreConfirm == "Y") {
              window.android.FN_LM_TEL(callString);
            }
            else {
              //location.href = 'tel:' + pTelNo;
              return true;
            }
          }

          return false;
        }
        else {
          //location.href = 'tel:' + pTelNo;
          return true;
        }
      }

      // FN_LM_CLOSE : 드로워메뉴 닫기 (닫기)
      function fnActionJob_Close() {
        ClickLog('MDC_1_4', '@(Request.Url.AbsolutePath.ToLower())');
