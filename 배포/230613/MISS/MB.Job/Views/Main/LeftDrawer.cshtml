      // 북마크 저장
      function fn_BookMarkSave(){
        if("@(ViewBag.ServiceType)" == "app"){
          $.ajax({
            type: "POST",
            url: "/Main/AjaxSaveBookMark",
            data: {
              'bookmark': $("#bookmark").val()
              , 'mid': "@(mid)"
            },
            async: false,
            success: function (result) {

              if(result=="0"){
                alert("즐겨찾기를 저장하는데 오류가 발생하였습니다.")
              }
              else if(result == "-1"){
                alert("기기정보가 올바르지 않습니다.");
              }
              else{
                //alert("성공");
              }
            },
            error: function (request, status, error) {
              //alert("code:" + request.status + "\n" + "message:" + request.responseText + "\n" + "error:" + error);
            }
          });
        }
        else{
          setCookie("bookmark", $("#bookmark").val(), 365);
        }
      }

      function fn_BookMark_MSG(type, ret) {

        if (ret == "memberinfo" || ret == "biz_change") {
          if(type == "biz"){
            alert("기업회원 서비스입니다.\n기업회원으로 로그인해 주세요.")
          }
