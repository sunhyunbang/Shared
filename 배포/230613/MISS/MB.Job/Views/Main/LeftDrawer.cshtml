        //var lastSelect = getCookie("lastSelect");
        var lastSelect = @(lastSelect);
        if (lastSelect.toString().length > 0)
          $("a[lastSelect=" + lastSelect + "]").trigger("click");
      });
      
      function MenuHeightInit(){
        if(window.matchMedia('(orientation:portrait').matches){
          var minheight =$(document).outerHeight() - ($('.directory__header').outerHeight() + $('.directory__bookmark').outerHeight() + 7);
          $('.menuBox, .tabCon').css('height', minheight);
        } else {
          var minheight =$(document).outerHeight() - ($('.directory__header').outerHeight() + $('.directory__bookmark').outerHeight() + 7);
          $('.menuBox, .tabCon').css('height', minheight);
        }
      }
      
      $(window).resize(function(){
        MenuHeightInit();
      })

      function LayerCont(type,className){
        if(type == 'open'){
          $('.'+className).show();
          $('body').addClass('scrollOff');
          allService();
        } else {
          $('.'+className).hide();
          $('body').removeClass('scrollOff');
        }
      }      

