            }
          }
          else {
            //ANDROID_APP
            if (strPalyStoreConfirm == "Y") {
              window.android.FN_LM_CLOSE(callString);
            }
            else if (strtStoreConfirm == "Y") {
              window.android.FN_LM_CLOSE(callString);
            }
            else {
              location.href = textArea.value;
            }
          }
        }
        else {
          location.href = textArea.value;
        }

        return false;
      }
