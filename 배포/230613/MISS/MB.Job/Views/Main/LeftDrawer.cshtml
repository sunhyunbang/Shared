            else if (strtStoreConfirm == "Y") {
              window.android.FN_LM_CLOSE_AND_MAIN_REDIRECT(callString);
            }
            else {
              location.href = textArea.value;
            }
          }
        }
        else {
          location.href = textArea.value;
        }

        return false;
