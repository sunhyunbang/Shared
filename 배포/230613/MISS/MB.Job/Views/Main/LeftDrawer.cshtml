      function tabMenu(e, num) {
        var num = num || 0;
        var menu = $(e).children('.tab-btn');
        var con = $(e + 'Con');
        var select = $(menu).eq(num);
        var i = num;

        select.addClass('on');
        con.eq(num).show();

        menu.click(function () {
          if (select !== null) {
            select.removeClass('on');
            con.eq(i).hide();
          }

          select = $(this);
          i = $(this).index();

          select.addClass('on');
          con.eq(i).show();
          MenuHeightInit();
        });
      }

      function menuMode(type){
        if(type == 'edit'){
          $('.btn-edit, .bookmark__listView').hide();
          $('.editbtn-wrap, .bookmark__listEdit').show();

          init_BookMark();
        } else {
          $('.btn-edit').show();
          $('.btn-edit, .bookmark__listView').show();
          $('.editbtn-wrap, .bookmark__listEdit').hide();
        }
        MenuHeightInit();
      }

      function allService(){
        $('.markBox').each(function(){
          var listNumber = $(this).find('a').length;
          var listRemainder= listNumber  % 4;
          if(listRemainder != 0){
            $(this).find('a').slice(-listRemainder, listNumber).addClass('noborder');
          }
        });
      }

      var prevScrollTop = 0;
      var nowScrollTop = 0;

      function wheelDelta(){
        return prevScrollTop - nowScrollTop > 0 ? 'up' : 'down'
      }

      $(window).on('scroll',function(){
        nowScrollTop = $(this).scrollTop();
        var menuScrollTop = $('.menuBox').offset().top;
        if(wheelDelta() == 'down'){
          if(nowScrollTop > 0 ){
            $('.directory__header').addClass('active');
          } else {
            $('.directory__header').removeClass('active');
          }
        }
        if(nowScrollTop >= menuScrollTop - 49){
          $('.tab').addClass('fixed');
        } else {
          $('.tab').removeClass('fixed');
        }
      });

      // 북마크 설정
      function fn_BookMark(obj){

        var name = $(obj).attr("name");
        if(!name){name = $(obj).attr("id");}

        // 북마크 해제
        if ($("[name=" + name + "]").hasClass("on") == true || $("[name=" + name + "]").length == 0){
          var arrBook = $("#bookmark").val().split(';');
          var arrBookMark = "";

          if(arrBook.length > 1){
            arrBookMark = $("#bookmark").val().replace(name + ";", "");
            arrBookMark = arrBookMark.replace(";" + name, "");
          }
          $("#bookmark").val(arrBookMark);
        }
        else{ // 북마크 설정
          if($("#bookmark").val()){
            if ($("#bookmark").val().split(";").length > 7){
              alert("즐겨찾기 메뉴는\n8개까지 등록 가능합니다.");
              return;
            }
            $("#bookmark").val($("#bookmark").val() + ";" + name);


            // 즐겨찾기를 정렬하여 저장하려면
            if ("@(bookMartSort)" == "1") {
              var arr_bookmark = $("#bookmark").val().split(';');
              var arr = new Array();
              var result = "";
              for (var i = 0; i < arr_bookmark.length; i++) {
                arr[i] = new Array();
                arr[i][0] = $("a[name=" + arr_bookmark[i] + "]").attr("no");
                arr[i][1] = arr_bookmark[i];
              }
              arr.sort();
              for (var i = 0; i < arr.length; i++) {
                if (i > 0) {
                  result = result + ';';
                }
                result = result + arr[i][1];
              }
              $("#bookmark").val(result);
            }

          }
          else{
            $("#bookmark").val(name);
          }
        }

        init_BookMark();
        fn_BookMarkSave();
        MenuHeightInit()
      }

