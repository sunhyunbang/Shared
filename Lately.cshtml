        $("#frmCS #REQ_WORK_TXT").val(rtnVal);

        var work_wf = $("#frmCS #REQ_WORK_WF").val();
        var work_wtext = $("#frmCS #REQ_WORK_WTEXT").val();
        rtnVal = valueSplitSetting(obj, work_wf, "val");
        $("#frmCS #REQ_WORK_WF").val(rtnVal);

        rtnVal = valueSplitSetting(obj, work_wtext, "valfulltxt");
        $("#frmCS #REQ_WORK_WTEXT").val(rtnVal);
      }
      else if ($(obj).attr("objname") == "dayweek") {
        var work_day_wk = $("#frmCS #REQ_WORK_DAY_WEEK").val();
        rtnVal = valueSplitSetting(obj, work_day_wk, "val");
        $("#frmCS #REQ_WORK_DAY_WEEK").val(rtnVal);

        $("#frmCS #REQ_WORK_DAY_WEEK_NEGO").val("@(REQ_WORK_DAY_WEEK_NEGO)");

        $("#TMP_WORK_DAY_WEEK").val($(obj).attr("val"));
        $("#TMP_WORK_DAY_WEEK_NEGO").val($("#REQ_WORK_DAY_WEEK_NEGO").val());
      }
      else if ($(obj).attr("objname") == "time") {
        $("#frmCS #REQ_WORK_TIME_TEXT").val(rtnVal);

        //기존꺼
        $("#TMP_WORK_TIME").val($(obj).attr("val"));
        $("#TMP_WORK_TIME_FROM").val($("#REQ_WORK_TIME_FROM").val());
        $("#TMP_WORK_TIME_TO").val($("#REQ_WORK_TIME_TO").val());
        $("#TMP_WORK_TIME_NEGO").val("0");
        //추가
