      }
    }
  }

  function valueSplitSetting(obj, val, type) {
    var arrval2 = val.split(",");
    var arrTemp2 = arrval2.slice();
    var arrLength2 = arrTemp2.length;
    var chnObjValue = ""
    if (val == "") {
      val = $.trim($(obj).attr(type));
      chnObjValue = val;
    } else {
      if (val.indexOf($.trim($(obj).attr(type))) != -1) {
        for (var i = 0; i < arrLength2; i++) {
          if ($.trim($(obj).attr(type)) == arrTemp2[i]) {
            arrTemp2.splice(i, 1);
            i--; arrLength2 = arrTemp2.length;// 삭제후 재실행(배열갯수 바뀜)
            break;
          }
        }

        for (var i = 0; i < arrLength2; i++) {
          if (chnObjValue == "") {
            chnObjValue = arrTemp2[i];
          } else {
            chnObjValue += "," + arrTemp2[i];
          }
        }
      } else {
        chnObjValue = val + "," + $.trim($(obj).attr(type));
      }
    }

    return chnObjValue;

