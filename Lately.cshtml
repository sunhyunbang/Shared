      })
    });
  });

  function getLatelyHistory(vCookieName) {
    var cookieHistory = "";
    var arrFullCookies = "";
    var arrTemp = "";
    var LatelyLI = "";
    var gbn = "";

    if (vCookieName == "@(COOKIE_TYPE)_LATELY_AREA") {
      gbn = "area";
    } else if (vCookieName == "@(COOKIE_TYPE)_LATELY_CATE") {
      gbn = "cate";
    }

    cookieHistory = unescape(getSearchCookie(vCookieName));

    if (cookieHistory != "") {
      arrFullCookies = cookieHistory.split("||");
      var minI = 0;

      if (arrFullCookies.length > 6) { minI = arrFullCookies.length - 6 }
      for (var ii = (arrFullCookies.length) - 1; ii >= minI; ii--) {
        if (arrFullCookies[ii] != "") {
          arrTemp = arrFullCookies[ii].split('|');
          console.log("  : " + arrFullCookies[ii]);
          console.log("arrTemp[0] : " + arrTemp[0] + " :: arrTemp[1] : " + arrTemp[1] + " :: arrTemp[2] : " + arrTemp[2] + " :: getSectionTitle : " + getSectionTitle(arrTemp[0]));
          var arrRQQ = arrTemp[1].split('^^');
          var code = arrRQQ[0];
          var txt = arrRQQ[1];
          var fulltxt = arrRQQ[1];
          txt = txt.replace(/==/g, " > ");


          LatelyLI += "<li class=\"lately-list__item\"><a href=\"javascript:;\" class=\"lately-list__item--box\" onclick=\"clickCheck(this, '" + gbn + "');\" val=\"" + code + "\"  valfulltxt=\"" + fulltxt + "\">" + txt + "</a></li>";
        }
      }
    }

    return LatelyLI;
  }

  // 최근 검색 쿠키 재설정
  function setLatelyCookie(vCookieName) {
    var cookieHistory = "";
    var arrFullCookies = "";
    var arrTemp = "";
    var strTxt = "";
    var arrTxt = "";

    cookieHistory = unescape(getSearchCookie(vCookieName));
    if (cookieHistory != "") {
      arrFullCookies = cookieHistory.split("||");
      var minI = 0;

      if (arrFullCookies.length > 6) { minI = arrFullCookies.length - 6 }
      for (var ii = (arrFullCookies.length) - 1; ii >= minI; ii--) {
        if (arrFullCookies[ii] != "") {
          arrTemp = arrFullCookies[ii].split('|');

          if (strTxt == "") {
            strTxt = arrTemp[1];
          } else {
            strTxt = strTxt + "," + arrTemp[1];
          }
        }
      }
      if (strTxt != "") {
        deleteSearchCookie('', '/', vCookieName);

        arrTxt = strTxt.split(',');
        for (var i = 5; i >= 0; i--) {
          //console.log("arrTxt[" + i + "] : " + arrTxt[i]);
          setSearchHistory('', '/', vCookieName, 365, '4', arrTxt[i]);
        }
      }
    }
  }

  function clickCheck(item, gbn) {
    if (gbn == "area" || gbn == "cate") {
      var obj = "";
      var maxCnt = 0;
      if (gbn == "area") {
        obj = "LatelyArea";
        maxCnt = "@(lCOUNT_AREA)";
      }
      else if (gbn == "cate") {
        obj = "LatelyCate";
        maxCnt = "@(lCOUNT_CATE)";
      }

      latelySelect(item, obj, maxCnt);


      var vaFlag = false;
      if (parseInt($("#" + obj + " .lately-list__item.active").length) <= parseInt(maxCnt)) {
        vaFlag = true;
      }

      if (vaFlag) {
        valueSetting(item, gbn);
      }
    }
    else {
      latelySelect(item, gbn, '0');
      valueSetting(item, gbn);
    }

    if (parseInt($("#TMP_AREA_COUNT").val()) == 0 && parseInt($("#TMP_CATE_COUNT").val()) == 0 && parseInt($("#TMP_WORK_TXT").val().length) == 0) {
      $(".bottom-area__result").addClass("disabled");
    } else {
      $(".bottom-area__result").removeClass("disabled");
    }
  }

  function valueSetting(obj, gbn) {
    var orgCodeValue = "";
    var orgTxtValue = "";
    var orgCnt = 0;

    if (gbn == "area" || gbn == "cate") {
      if (gbn == "area") {
        orgCodeValue = $("#TMP_AREA").val();
        orgTxtValue = $("#TMP_AREA_TXT").val();
        orgCnt = $("#TMP_AREA_COUNT").val();
      }
      else if (gbn == "cate") {
        orgCodeValue = $("#TMP_CATE").val();
        orgTxtValue = $("#TMP_CATE_TXT").val();
        orgCnt = $("#TMP_CATE_COUNT").val();
      }

      var arrCodeValue = orgCodeValue.split(",");
      var arrTxtValue = orgTxtValue.split(",");
      var arrCodeTemp = arrCodeValue.slice();
      var arrCodeLength = arrCodeTemp.length;
      var arrTxtTemp = arrTxtValue.slice();
      var chnCodeValue = "";
      var chnTxtValue = "";
      var iflag = true;

      if (orgCodeValue == "") {
        chnCodeValue = $.trim($(obj).attr("val"));
        chnTxtValue = $.trim($(obj).attr("valfulltxt"));
        orgCnt = parseInt(orgCnt) + 1;
      } else {
        if (orgCodeValue.indexOf($.trim($(obj).attr("val"))) != -1) {
          for (var i = 0; i < arrCodeLength; i++) {
            if ($.trim($(obj).attr("val")) == $.trim(arrCodeTemp[i])) {
              arrCodeTemp.splice(i, 1);
              i--; arrCodeLength = arrCodeTemp.length;

              arrTxtTemp.splice(i, 1);
              i--;
              break;
            }
          }

          for (var i = 0; i < arrCodeLength; i++) {
            if (chnCodeValue == "") {
              chnCodeValue = arrCodeTemp[i];
              chnTxtValue = arrTxtTemp[i];
            } else {
              chnCodeValue += "," + arrCodeTemp[i];
              chnTxtValue += "," + arrTxtTemp[i];
            }
          }

          orgCnt = parseInt(orgCnt) - 1;
        } else {
          chnCodeValue = orgCodeValue + "," + $.trim($(obj).attr("val"));
          chnTxtValue = orgTxtValue + "," + $.trim($(obj).attr("valfulltxt"));
          orgCnt = parseInt(orgCnt) + 1;
        }
      }

      if (gbn == "area") {
        $("#TMP_AREA").val(chnCodeValue);
        $("#TMP_AREA_TXT").val(chnTxtValue);
        $("#TMP_AREA_COUNT").val(orgCnt);
      }
      else if (gbn == "cate") {
        $("#TMP_CATE").val(chnCodeValue);
        $("#TMP_CATE_TXT").val(chnTxtValue);
        $("#TMP_CATE_COUNT").val(orgCnt);
      }
    }
    else if (gbn == "work") {
      var strVal = $("#TMP_WORK_TXT").val();
