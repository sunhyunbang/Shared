      var arrTemp = arrVal.slice();
      var arrLength = arrTemp.length;
      var rtnVal = "";
      var wCodeVal = "";

      if (strVal == "") {
        rtnVal = $.trim($(obj).attr("valfulltxt"));

      } else {
        if (strVal.indexOf($.trim($(obj).attr("valfulltxt"))) != -1) {
          for (var i = 0; i < arrLength; i++) {
            if ($.trim($(obj).attr("valfulltxt")) == arrTemp[i]) {
              arrTemp.splice(i, 1);
              i--; arrLength = arrTemp.length;// 삭제후 재실행(배열갯수 바뀜)
              break;
            }
          }

          for (var i = 0; i < arrLength; i++) {
            if (rtnVal == "") {
              rtnVal = arrTemp[i];
            } else {
              rtnVal += "," + arrTemp[i];
            }
          }
        } else {
          rtnVal = strVal + "," + $.trim($(obj).attr("valfulltxt"));
        }
      }

      $("#TMP_WORK_TXT").val(rtnVal);

      if ($(obj).attr("objname") == "payf") {
        $("#frmCS #REQ_WORK_TXT").val(rtnVal);
        $("#frmCS #REQ_WORK_PAYF").val($(obj).attr("val"));
        $("#frmCS #REQ_WORK_PAYAMOUNT").val("@(REQ_WORK_PAYAMOUNT)");
        $("#frmCS #REQ_WORK_PAY_TEXT").val("@(REQ_WORK_PAY_TEXT)");
      }
